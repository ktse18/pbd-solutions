public class YourInitials
{
	public static void main(String[] args)
	{
		System.out.println("K   K  TTTTT");  
		System.out.println("K  K     T");
		System.out.println("K K      T");
		System.out.println("KK       T");
		System.out.println("K K      T");
		System.out.println("K  K     T");
		System.out.println("K   K    T");
	}
}
